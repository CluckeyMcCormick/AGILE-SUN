<p id="notice"><%= notice %></p>

<h1>Listing Rated Talents</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Talent</th>
      <th>Rating</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rated_talents.each do |rated_talent| %>
      <tr>
        <td><%= User.find(rated_talent.user_id).name %></td>
        <td><%= Talent.find(rated_talent.talent_id).name %></td>
        <td><%= rated_talent.rating_value %></td>
        <td><%= link_to 'Show', rated_talent %></td>
        <td><%= link_to 'Edit', edit_rated_talent_path(rated_talent) %></td>
        <td><%= link_to 'Destroy', rated_talent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rated talent', new_rated_talent_path %>
